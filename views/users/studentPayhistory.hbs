                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="d-flex align-items-center">
                                                        <small class="text-uppercase text-muted mr-2">{{this.program.program}}</small>
                                                    </div>
                                                </td>
                                                <td class="text-center">
                                                    <div class="d-flex align-items-center">
                                                        <small class="text-uppercase text-muted mr-2"></small>
                                                        <small class="text-uppercase">â‚¹<span
                                                                class="js-lists-values-amount">{{this.finalAmount}}</span>INR</small>
                                                    </div>
                                                </td>
                                                <td class="text-center">
                                                    <div class="d-flex align-items-center">
                                                        <small class="text-uppercase text-muted mr-2">{{this.paymentStatus}}</small>
                                                        <i class="material-icons text-success md-18 mr-2"></i>
                                                    </div>
                                                </td>
                                                <td class="text-right">
                                                    <div class="d-flex align-items-center text-right">
                                                        <small class="text-uppercase text-muted mr-2">Date</small>
                                                        <small class="text-uppercase js-lists-values-date">{{format this.updatedAt}}</small>
                                                    </div>
                                                </td>
                                            </tr>
                                            {{/each}}
